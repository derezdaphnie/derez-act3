\r\n            <div>\r\n              <p class=\"item-name\">{{ item.name }}</p>\r\n              <p class=\"item-price\">$ {{ item.price }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"button-group\">\r\n          <button @click=\"addToCart(item)\" class=\"add-to-cart-btn\">Add to Cart</button>\r\n          <button @click=\"updateItem(index)\" class=\"update-btn\">Update</button>\r\n          <button @click=\"removeItem(index)\" class=\"remove-btn\">Remove</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <br>\r\n    <button @click=\"addItem\" class=\"add-item-btn\">Add New Item</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      items: [\r\n        { name: 'Bananacue', price: 10, image: 'bananacue.jpg' },\r\n        { name: 'Buko Pie', price: 20, image: 'buko pie.jpg' },\r\n        { name: 'Empenada', price: 15, image: 'empenada.jpg' },\r\n        { name: 'Kutsinta', price: 11, image: 'kutsinta.jpg' },\r\n        { name: 'Maruya', price: 32, image: 'maruya.jpg' },\r\n        { name: 'Pansit', price: 23, image: 'pansit.jpg' },\r\n        { name: 'Puto', price: 23, image: 'puto.jpg' },\r\n        { name: 'Suman', price: 23, image: 'suman.jpg' },\r\n        { name: 'Turon', price: 23, image: 'turon.jpg' },\r\n        { name: 'Halo-Halo', price: 23, image: 'halohalo.jpg' }\r\n      ],\r\n      searchQuery: ''\r\n    };\r\n  },\r\n  computed: {\r\n    filteredItems() {\r\n      return this.items.filter(item =>\r\n        item.name.toLowerCase().includes(this.searchQuery.toLowerCase())\r\n      );\r\n    }\r\n  },\r\n  methods: {\r\n    addToCart(item) {\r\n      this.$emit('add-to-cart', item);\r\n    },\r\n    updateItem(index) {\r\n      const item = this.items[index];\r\n      const newName = prompt('Enter the new name:', item.name);\r\n      const newPrice = parseFloat(prompt('Enter the new price:', item.price));\r\n      \r\n      if (newName && !isNaN(newPrice)) {\r\n        item.name = newName;\r\n        item.price = newPrice;\r\n      }\r\n    },\r\n    removeItem(index) {\r\n      this.items.splice(index, 1);\r\n    },\r\n    addItem() {\r\n      const name = prompt('Enter the name for the new item:');\r\n      const price = parseFloat(prompt('Enter the price for the new item:'));\r\n      let image = prompt('Enter the filename of the image for the new item:');\r\n      \r\n      // Ensure the image filename has a valid extension\r\n      const isValidExtension = /\\.(jpg|png)$/i.test(image);\r\n      \r\n      if (!isValidExtension) {\r\n        alert('Please enter a valid image filename with extension .jpg or .png');\r\n        return;\r\n      }\r\n      \r\n      if (name && !isNaN(price) && image) {\r\n        this.items.push({ name, price, image });\r\n      }\r\n    },\r\n    getImagePath(image) {\r\n      return `/img/${image}`;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.item-list {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  margin-top: 20px;\r\n}\r\n\r\n.item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 10px;\r\n  border-bottom: 1px solid #ccc;\r\n  width: 80%;\r\n}\r\n\r\n.item-details {\r\n  flex: 1;\r\n}\r\n\r\n.item-info {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.item-image {\r\n  width: 50px;\r\n  height: 50px;\r\n  border-radius: 50%;\r\n  margin-right: 10px;\r\n}\r\n\r\n.item-name {\r\n  margin: 0;\r\n  font-weight: bold;\r\n}\r\n\r\n.item-price {\r\n  margin: 0;\r\n}\r\n\r\n.button-group {\r\n  display: flex;\r\n  gap: 10px;\r\n}\r\n\r\n.add-to-cart-btn, .update-btn, .remove-btn, .add-item-btn {\r\n  background-color: #4caf50;\r\n  color: white;\r\n  border: none;\r\n  padding: 8px 16px;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.add-to-cart-btn:hover, .update-btn:hover, .remove-btn:hover, .add-item-btn:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\n.search-input {\r\n  margin-bottom: 20px;\r\n  text-align: center;\r\n  width: 20%; /* Set the width to 80% of its container */\r\n}\r\n</style>\r\n"],"mappings":";;;EAGSA,KAAK,EAAC;AAAW;;EAEbA,KAAK,EAAC;AAAc;;EAClBA,KAAK,EAAC;AAAW;mBANhC;;EASiBA,KAAK,EAAC;AAAW;;EACjBA,KAAK,EAAC;AAAY;;EAItBA,KAAK,EAAC;AAAc;mBAdjC;mBAAA;oBAAA;iEAqBIC,mBAAA,CAAI;;uBApBNC,mBAAA,CAsBM,c,gBArBJD,mBAAA,CAAkG;IAA3FE,IAAI,EAAC,MAAM;IAFtB,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAEgCC,KAAA,CAAAC,WAAW,GAAAF,MAAA;IAAEG,WAAW,EAAC,uBAAuB;IAACR,KAAK,EAAC;iDAAvDM,KAAA,CAAAC,WAAW,E,GACvCN,mBAAA,CAiBM,OAjBNQ,UAiBM,I,kBAhBJP,mBAAA,CAeMQ,SAAA,QAnBZC,WAAA,CAImCC,QAAA,CAAAC,aAAa,EAJhD,CAImBC,IAAI,EAAEC,KAAK;yBAAxBb,mBAAA,CAeM;MAfuCc,GAAG,EAAED,KAAK;MAAEf,KAAK,EAAC;QAC7DC,mBAAA,CAQM,OARNgB,UAQM,GAPJhB,mBAAA,CAMM,OANNiB,UAMM,GALJjB,mBAAA,CAAyE;MAAnEkB,GAAG,EAAEP,QAAA,CAAAQ,YAAY,CAACN,IAAI,CAACO,KAAK;MAAGC,GAAG,EAAC,YAAY;MAACtB,KAAK,EAAC;4BAPxEuB,UAAA,GAQYtB,mBAAA,CAGM,cAFJA,mBAAA,CAAwC,KAAxCuB,UAAwC,EAAAC,gBAAA,CAAhBX,IAAI,CAACY,IAAI,kBACjCzB,mBAAA,CAA4C,KAA5C0B,UAA4C,EAAtB,IAAE,GAAAF,gBAAA,CAAGX,IAAI,CAACc,KAAK,iB,OAI3C3B,mBAAA,CAIM,OAJN4B,UAIM,GAHJ5B,mBAAA,CAA6E;MAApE6B,OAAK,EAAAzB,MAAA,IAAEO,QAAA,CAAAmB,SAAS,CAACjB,IAAI;MAAGd,KAAK,EAAC;OAAkB,aAAW,iBAf9EgC,UAAA,GAgBU/B,mBAAA,CAAqE;MAA5D6B,OAAK,EAAAzB,MAAA,IAAEO,QAAA,CAAAqB,UAAU,CAAClB,KAAK;MAAGf,KAAK,EAAC;OAAa,QAAM,iBAhBtEkC,UAAA,GAiBUjC,mBAAA,CAAqE;MAA5D6B,OAAK,EAAAzB,MAAA,IAAEO,QAAA,CAAAuB,UAAU,CAACpB,KAAK;MAAGf,KAAK,EAAC;OAAa,QAAM,iBAjBtEoC,WAAA,E;oCAqBIC,WAAI,EACJpC,mBAAA,CAAmE;IAA1D6B,OAAK,EAAA1B,MAAA,QAAAA,MAAA,UAAAkC,IAAA,KAAE1B,QAAA,CAAA2B,OAAA,IAAA3B,QAAA,CAAA2B,OAAA,IAAAD,IAAA,CAAO;IAAEtC,KAAK,EAAC;KAAe,cAAY,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}